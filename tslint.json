{
  "extends": "tslint:recommended",
  "rules": {
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "interface-name": [
      false
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "quotemark": [
      true, "single", "avoid-escape"
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-var-requires": false
  }
}